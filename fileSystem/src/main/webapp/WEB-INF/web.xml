<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>com.lemonzuo.servlet.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileListServlet</servlet-name>
        <servlet-class>com.lemonzuo.servlet.FileListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/fileUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileListServlet</servlet-name>
        <url-pattern>/fileList</url-pattern>
    </servlet-mapping>
</web-app>
